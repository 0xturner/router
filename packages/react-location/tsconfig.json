{
  "compilerOptions": {
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": ["ESNext", "dom"],
    "target": "es5",
    "noEmit": true,
    "noImplicitAny": true,
    "noImplicitReturns": false,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noUncheckedIndexedAccess": true,
    "skipLibCheck": true,
    "strict": true,
    "types": ["jest", "node"],
    "paths": {
      "react-location": ["./src/index.tsx"]
    }
  },
  "include": ["./src", "src/global.d.ts"]
}
